<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <display-name>MusicProject</display-name>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>com.musicproject.servlet.InsertServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.musicproject.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>getmusic</servlet-name>
        <servlet-class>com.musicproject.servlet.MusicServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getmusic</servlet-name>
        <url-pattern>/getmusic</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>contact</servlet-name>
        <servlet-class>com.musicproject.servlet.PurchaseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>contact</servlet-name>
        <url-pattern>/contact</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>buied</servlet-name>
        <servlet-class>com.musicproject.servlet.UserBuiedServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>buied</servlet-name>
        <url-pattern>/buied</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>upload</servlet-name>
        <servlet-class>com.musicproject.servlet.UploadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>upload</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>buymusic</servlet-name>
        <servlet-class>com.musicproject.servlet.InsertPurchaseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>buymusic</servlet-name>
        <url-pattern>/buymusic</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>down</servlet-name>
        <servlet-class><com class="musicproject"></com>servlet.DownloadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>down</servlet-name>
        <url-pattern>/down</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.htm</welcome-file>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file>default.html</welcome-file>
        <welcome-file>default.htm</welcome-file>
        <welcome-file>default.jsp</welcome-file>
    </welcome-file-list>
</web-app>